CLASS net/minecraft/world/level/levelgen/synth/BlendedNoise
	COMMENT This class wraps three individual perlin noise octaves samplers.
	COMMENT It computes the octaves of the main noise, and then uses that as a linear interpolation value between the minimum and maximum limit noises.
	METHOD <init> (Lnet/minecraft/world/level/levelgen/RandomSource;Lnet/minecraft/world/level/levelgen/NoiseSamplingSettings;II)V
		ARG 1 randomSource
		ARG 2 noiseSamplingSettings
		ARG 3 cellWidth
		ARG 4 cellHeight
	METHOD <init> (Lnet/minecraft/world/level/levelgen/synth/PerlinNoise;Lnet/minecraft/world/level/levelgen/synth/PerlinNoise;Lnet/minecraft/world/level/levelgen/synth/PerlinNoise;Lnet/minecraft/world/level/levelgen/NoiseSamplingSettings;II)V
		ARG 1 minLimitNoise
		ARG 2 maxLimitNoise
		ARG 3 mainNoise
		ARG 4 noiseSamplingSettings
		ARG 5 cellWidth
		ARG 6 cellHeight
